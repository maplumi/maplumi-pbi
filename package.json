  "scripts": {
    "pbiviz": "npx pbiviz",
    "start": "npx pbiviz start",
    "package": "npm run sync-version && npx pbiviz package",
    "sync-version": "tsx scripts/sync-version.ts",
    "version:patch": "tsx scripts/increment-version.ts patch",
    "version:minor": "tsx scripts/increment-version.ts minor",
    "version:major": "tsx scripts/increment-version.ts major",
